[user]
  name = Josh Ponelat
  email = josh@ponelat.com
[alias]
  re = reset
  rh = reset HEAD~
  ref = reflog
  rf= reflog -n 10
  co = checkout
  cb = checkout -b
  ct = checkout --theirs
  ci = commit --verbose
  ca = commit --all --verbose
  cm = commit -m
  amend = commit --amend --verbose
  amendd = commit --amend --verbose -m ''
  di = diff
  dc = diff --staged
  ds = diff --stat
  aa = add --all
  ap = add --all -p
  br = branch
  bc = branch --contains
  bm = branch --move
  ff = merge --ff-only
  lr = log --oneline --left-right
  right = log --oneline --right-only 
  left = log --oneline --left-only 
  ri = rebase -i
  ra = rebase --abort
  rc = rebase --continue
  one = log --oneline
  l = "!. ~/.githelpers && pretty_git_log"
[push]
  default = simple
